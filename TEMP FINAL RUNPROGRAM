						else{
							bw.write("false");
						}
						bw.append("");//same as before
						bw.flush();//same as before
						bw.newLine();//same as before

						bw.write( Integer.toString(Central.get(i).get(0).getIDNum())); //changes the id num to a string and writes it to the text file
						bw.append("");//same as before
						bw.flush();//same as before
						bw.newLine();//same as before

						bw.write(Integer.toString(Central.get(i).get(0).getParentID()));//changes the parentID to a string and writes it to the text file
						bw.append("");//same as before
						bw.flush();//same as before
						bw.newLine();//same as before
					}


